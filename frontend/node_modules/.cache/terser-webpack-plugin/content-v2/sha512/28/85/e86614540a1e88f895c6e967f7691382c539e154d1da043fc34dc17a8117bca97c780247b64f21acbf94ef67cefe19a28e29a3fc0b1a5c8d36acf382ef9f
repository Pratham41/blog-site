      post && (\n          <Row>\n            <Col md={6}>\n              <ListGroup variant='flush'>\n                <ListGroup.Item>\n                  <h3>{post.title}</h3>\n                </ListGroup.Item>\n\n                <ListGroup.Item>Name : {post.name}</ListGroup.Item>\n                <ListGroup.Item>Description : {post.content}</ListGroup.Item>\n              </ListGroup>\n            </Col>\n            <Col md={6}>\n              <ListGroup>\n                <ListGroup.Item className='bg-dark text-warning'>\n                  <h5>Comments</h5>\n                </ListGroup.Item>\n                {post.comments.length > 0 ? (\n                  post.comments.map((x) => (\n                    <ListGroup.Item key={x._id}>\n                      <Row>\n                        <Col md={3}>\n                          <i className='fas fa-user-circle text-info'>\n                            {'  '}\n                            {x.user}\n                          </i>\n                        </Col>\n                        <Col md={9}>\n                          <p className='text-success'>{x.comment}</p>\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))\n                ) : (\n                  <ListGroup.Item>\n                    <Row>\n                      <Col md={12}>\n                        <p>No Comments</p>\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>\n                )}\n                <ListGroup.Item>\n                  <Button\n                    onClick={addCommentHandler}\n                    className='btn btn-md btn-success'\n                    type='button'\n                  >\n                    Add a Comment\n                  </Button>\n                </ListGroup.Item>\n                {/* Handle comment button clicked */}\n                {commentVisible && (\n                  <ListGroup.Item>\n             